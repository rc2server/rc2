{
	"apiVersion": "batch/v1",
	"kind": "Job",
	"metadata": {
		"name": "compute-{{ sessionId }}",
		"labels": {
			"workspace": "{{ wspaceId }}",
			"sessionId": "{{ sessionId }}",
			"compute": true
			}
		},
	"spec": {
			"template": {
			  "metadata": { "labels": { 
			    "workspace": "{{ wspaceId }}", 
			    "sessionId": "{{ sessionId }}" 
			  }},
				"spec": {
					"restartPolicy": "Never",
					"containers": [{
						"name": "compute-{{ sessionId }}",
						"image": "{{ computeImage }}",
						"ports": [{
							"containerPort": 7714
							}],
						"command": ["/rc2compute/rserver"],
          	"imagePullPolicy": "Always"
					}],
					"imagePullSecrets": [{ "name": "regcred" }]
				}
			}
		}
}
